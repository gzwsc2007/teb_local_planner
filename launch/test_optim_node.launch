<launch>
	
  	<!--- Run optimization test node -->
   	<node pkg="teb_local_planner" type="test_optim_node" name="test_optim_node" output="screen">
   		<rosparam param="footprint_model/line_start">[-2.0, 0.0]</rosparam>
   		<rosparam param="footprint_model/line_end">[1.0, 0.0]</rosparam>
   		<param name="footprint_model/line_separation" value="2"/>
   		<param name="footprint_model/type" value="ski_lines"/>
   	</node>

  	<!-- RVIZ -->
  	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find teb_local_planner)/cfg/rviz_test_optim.rviz" />

</launch>

